	<!-- 서버타입에 따른 propertie 파일 설정 -->
	<util:properties id="prop" 		location="classpath:properties/prop.properties" />
	<util:properties id="jdbc" 		location="classpath:properties/jdbc.properties"/>
	<util:properties id="sap" 		location="classpath:properties/sap.properties"/>
	
</beans>
